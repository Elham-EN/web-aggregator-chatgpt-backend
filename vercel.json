{
  "version": 2,
  "builds": [{ "src": "index.js", "use": "@vercel/node" }],
  "routes": [
    { "src": "api/(.*)", "dest": "index.js" },
    { "src": "api/(.*)", "headers": { "Access-Control-Allow-Origin": "*" } }
  ],
  "env": {
    "SERVER_PORT": "80",
    "CHAT_GPT_EMAIL": "@vercel-secrets.CHAT_GPT_EMAIL",
    "CHAT_GPT_PASSWORD": "@vercel-secrets.CHAT_GPT_PASSWORD"
  }
}
